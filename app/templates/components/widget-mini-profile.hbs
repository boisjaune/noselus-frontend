{{#link-to 'politician' model}}
  <div class="mini-profile thumbnail centered-background" {{bind-attr style=model.backgroundThumb}}>
    <div {{bind-attr class='model.assembly :img-circle :thumbnail'}}></div>
    <div class="caption">
      {{model.fullName}}
      {{#if model.partyName}}
        ({{model.partyName}})
      {{/if}}
    </div>
  </div>
{{/link-to}}


